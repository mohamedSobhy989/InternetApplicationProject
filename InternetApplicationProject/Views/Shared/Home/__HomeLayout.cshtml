<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="shortcut icon" type="image/icon" href="~/Image/favicon.png" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Font awesome css file-->
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <!-- Superslide css file-->
    <link href="~/Content/superslides.css" rel="stylesheet" />
    <!-- Slick slider css file -->
    <link href="~/Content/slick.css" rel="stylesheet" />
    <!-- Circle counter cdn css file -->
    <link rel='stylesheet prefetch' href='https://cdn.rawgit.com/pguso/jquery-plugin-circliful/master/css/jquery.circliful.css'>
    <!-- smooth animate css file -->
    <link href="~/Content/animate.css" rel="stylesheet" />
    <!-- gallery slider css -->
    <link href="~/Content/jquery.tosrus.all.css" rel="stylesheet" />
    <!-- Default Theme css file -->
    <link href="~/Content/default-theme.css" rel="stylesheet" />
    <!-- Main structure css file -->
    <link href="~/Content/HomeStyle.css" rel="stylesheet" />

    <!-- Google fonts -->
    <link href="~/Content/Merriweather.css" rel="stylesheet" />
    <link href="~/Content/Varela.css" rel="stylesheet" />
</head>
<body>
    <!--=========== BEGIN HEADER SECTION ================-->
    <header id="header">
        <!-- BEGIN MENU -->
        <div class="menu_area">
            <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
                <div class="container">
                    <div class="navbar-header">
                        <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <!-- LOGO -->
                        <!-- TEXT BASED LOGO -->
                        <a class="navbar-brand" href="index.html">COZA<span>STORE</span></a>
                        <!-- IMG BASED LOGO  -->
                        <!-- <a class="navbar-brand" href="index.html"><img src="img/logo.png" alt="logo"></a>  -->

                    </div>
                    <div id="navbar" class="navbar-collapse collapse">
                        <ul id="top-menu" class="nav navbar-nav navbar-right main-nav">
                            <li class="active"><a href="index.html">Home</a></li>
                            <li><a href="course-archive.html">Projects</a></li>
                            <li><a href="scholarship.html">Blog</a></li>
                            <li><a href="contact.html">Contact</a></li>
                            <li><a href="gallery.html">Login</a></li>
                            <li><a href="blog-archive.html">SignUp</a></li>
                        </ul>
                    </div><!--/.nav-collapse -->
                </div>
            </nav>
        </div>
        <!-- END MENU -->
    </header>
    <!--=========== END HEADER SECTION ================-->
    <!--=========== BEGIN SLIDER SECTION ================-->
    <section id="slider">
        <div class="row">
            <div class="col-lg-12 col-md-12">
                <div class="slider_area">
                    <!-- Start super slider -->
                    <div id="slides">
                        <ul class="slides-container">
                            <li>
                                <img src="~/Image/2.jpg" alt="img">
                                <div class="slider_caption">
                                    <h2>Largest & Beautiful Campany</h2>
                                    <p> Here you can add new project and we will market it , and you will choose the director by your selve according to his profile.</p>
                                    <a class="slider_btn" href="#">Know More</a>
                                </div>
                            </li>
                            <!-- Start single slider-->
                            <li>
                                <img src="~/Image/login-bg.jpg" alt="img">
                                <div class="slider_caption slider_right_caption">
                                    <h2>Flexable to deal with Site </h2>
                                    <p>
                                        Here you can add your project and control it (remove it or update )
                                        . Just you, can select your director that will work on your project
                                        you can see statistical to your projects.
                                    </p>
                                    <a class="slider_btn" href="#">Know More</a>
                                </div>
                            </li>

                        </ul>
                        <nav class="slides-navigation">
                            <a href="#" class="next"></a>
                            <a href="#" class="prev"></a>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--=========== END SLIDER SECTION ================-->
    <!--=========== BEGIN ABOUT US SECTION ================-->
    <section id="contact" style="background-color: #DDD">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <div class="title_area">
                        <h2 class="title_two">We are CozaStore Campany </h2>
                        <span></span>
                        <p>
                            We are a company that make markiting to your project .
                            Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book
                        </p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="contact_form wow fadeInLeft">
                        <p style="color:red;">@ViewBag.errorMessege</p>
                        @using (Html.BeginForm("AddProduct", "OurHome", FormMethod.Post, new { enctype = "multipart/form-data" }))
                        {
                            <input type="text" class="wp-form-control wpcf7-text" placeholder="Your Project name">
                            <input type="mail" class="wp-form-control wpcf7-email" placeholder="Project Description">
                            <textarea class="wp-form-control wpcf7-textarea" style="width: 81%;margin-left: 14px;" cols="30" rows="10" placeholder="Project Description "></textarea>
                            <div class="form-group row" style="width:80%;margin-left: 50px;">
                                <div class="col-sm-10">
                                    <label>Upload Your Photo</label>
                                    <input type="file" name="ImageFile" required />
                                </div>
                            </div>
                            <input type="submit" value="Submit" class="wpcf7-submit">
                        }
                        </div>
                </div>

            </div>
        </div>
    </section>
    <!--=========== END ABOUT US SECTION ================-->



    
    <div>
        @RenderBody()
    </div>








    <!--=========== BEGIN FOOTER SECTION ================-->
    <footer id="footer">
        <!-- Start footer top area -->
        <div class="footer_top">
            <div class="container">
                <div class="row">
                    <div class="col-ld-3  col-md-3 col-sm-3">
                        <div class="single_footer_widget">
                            <h3>About Us</h3>
                            <p>
                                We are a company that make markiting to your project.
                                Here you can add new project and we will market it , and you will choose the director by your selve according to his profile.
                            </p>
                        </div>
                    </div>
                    <div class="col-ld-3  col-md-3 col-sm-3">
                        <div class="single_footer_widget">
                            <h3>Community</h3>
                            <ul class="footer_widget_nav">
                                <li><a href="#">Our Directors</a></li>
                                <li><a href="#">Our Customers</a></li>
                                <li><a href="#">Our Team</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-ld-3  col-md-3 col-sm-3">
                        <div class="single_footer_widget">
                            <h3>Others</h3>
                            <ul class="footer_widget_nav">
                                <li><a href="#">Link 1</a></li>
                                <li><a href="#">Link 2</a></li>
                                <li><a href="#">Link 3</a></li>
                                <li><a href="#">Link 4</a></li>
                                <li><a href="#">Link 5</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-ld-3  col-md-3 col-sm-3">
                        <div class="single_footer_widget">
                            <h3>Social Links</h3>
                            <ul class="footer_social">
                                <li><a data-toggle="tooltip" data-placement="top" title="Facebook" class="soc_tooltip" href="#"><i class="fa fa-facebook"></i></a></li>
                                <li><a data-toggle="tooltip" data-placement="top" title="Twitter" class="soc_tooltip" href="#"><i class="fa fa-twitter"></i></a></li>
                                <li><a data-toggle="tooltip" data-placement="top" title="Google+" class="soc_tooltip" href="#"><i class="fa fa-google-plus"></i></a></li>
                                <li><a data-toggle="tooltip" data-placement="top" title="Linkedin" class="soc_tooltip" href="#"><i class="fa fa-linkedin"></i></a></li>
                                <li><a data-toggle="tooltip" data-placement="top" title="Youtube" class="soc_tooltip" href="#"><i class="fa fa-youtube"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End footer top area -->
        <!-- Start footer bottom area -->
        <div class="footer_bottom">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-6">
                        <div class="footer_bootomLeft">
                            <p> Copyright &copy; All Rights Reserved</p>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6">
                        <div class="footer_bootomRight">
                            <p>Designed by <a href="http://wpfreeware.com/" rel="nofollow">Wpfreeware.com</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End footer bottom area -->
    </footer>
    <!--=========== END FOOTER SECTION ================-->
    <!-- initialize jQuery Library -->
    <script src="~/Scripts/jquery.min.js"></script>
    <!-- Preloader js file -->
    <script src="~/Scripts/queryloader2.min.js"></script>
    <!-- For smooth animatin  -->
    <script src="~/Scripts/wow.min.js"></script>
    <!-- Bootstrap js -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- slick slider -->
    <script src="~/Scripts/slick.min.js"></script>
    <!-- superslides slider -->
    <script src="~/Scripts/jquery.easing.1.3.js"></script>
    <script src="~/Scripts/jquery.animate-enhanced.min.js"></script>
    <script src="~/Scripts/jquery.superslides.min.js"></script>
    <!-- for circle counter -->
    <script src="~/Scripts/jquery.circliful.min.js"></script>
    <!-- Gallery slider -->
    <script src="~/Scripts/jquery.tosrus.min.all.js"></script>

    <!-- Custom js-->
    <script src="~/Scripts/custom.js"></script>

</body>
</html>
